{
  "name": "log-analyzer",
  "version": "1.0.0",
  "description": "A modern web application for uploading, analyzing, and visualizing logs from password-protected or unprotected ZIP files.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install"
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "better-sqlite3": "^12.4.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "log-analyzer",
    "zip",
    "file-upload",
    "react",
    "express"
  ],
  "author": "Log Analyzer Team",
  "license": "MIT"
}
